<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter Game Location Details</h1>
  <br/>
  <%= simple_form_for @place do |f|%>
    <%= f.input :name %>
    <br/>
    <%= f.input :address %>
    <br/>
    <%= f.input :description %>
    <br/>
    <%= f.submit 'Add Place', class: 'btn btn-success' %>
  <% end %>
</div>